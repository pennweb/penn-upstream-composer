{
    "name": "pantheon-upstreams/drupal-project",
    "description": "Install Drupal 8 with Composer on Pantheon.",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        }
    ],
    "require": {
        "php": ">=7.3",
        "composer/installers": "^1.8",
        "cweagans/composer-patches": "^1.0",
        "drupal/adminimal_admin_toolbar": "^1.10",
        "drupal/adminimal_theme": "^1.5",
        "drupal/config_direct_save": "^1.0",
        "drupal/config_split": "^1.7",
        "drupal/console": "^1",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-recommended": "^9",
        "drupal/ctools": "3.9.0",
        "drupal/devel": "^4.1",
        "drupal/dropzonejs": "^2.5",
        "drupal/emptyparagraphkiller": "^1.0@alpha",
        "drupal/entity_browser": "^2.5",
        "drupal/entity_embed": "^1.1",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/externalauth": "^1.3",
        "drupal/fences": "^2.0@RC",
        "drupal/field_hidden": "^2.0",
        "drupal/google_tag": "^1.4",
        "drupal/image_widget_crop": "^2.3",
        "drupal/imagemagick": "^3.1",
        "drupal/inline_entity_form": "1.0-rc.9",
        "drupal/media_entity_browser": "^2.0@alpha",
        "drupal/meta_position": "^1.0",
        "drupal/metatag": "^1.14",
        "drupal/migrate_plus": "^5.3",
        "drupal/migrate_tools": "^4.5",
        "drupal/multiple_fields_remove_button": "^1.0@alpha",
        "drupal/paragraphs": "^1.11",
        "drupal/pathauto": "^1.8",
        "drupal/protected_pages": "^1.4",
        "drupal/redis": "^1.4",
        "drupal/simplesamlphp_auth": "^3.2",
        "drupal/simplify_menu": "^2.0",
        "drupal/structure_sync": "^2.0",
        "drupal/twig_tweak": "^2.6",
        "drupal/xmlsitemap": "^1.0",
        "drush-ops/behat-drush-endpoint": "^9.3",
        "drush/drush": "^10",
        "enyo/dropzone": "^5.7",
        "oomphinc/composer-installers-extender": "^2.0",
        "pantheon-systems/drupal-integrations": "^9",
        "pantheon-systems/quicksilver-pushback": "^2",
        "pantheon-upstreams/upstream-configuration": "1.0.1",
        "rvtraveller/qs-composer-installer": "^1.1",
        "zaporylie/composer-drupal-optimizations": "^1.2"
    },
    "conflict": {
            "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "rm -rf vendor/simplesamlphp/simplesamlphp/config && ln -sf ../../../private/simplesamlphp/config vendor/simplesamlphp/simplesamlphp/config",
            "rm -rf vendor/simplesamlphp/simplesamlphp/cert && ln -sf ../../../private/simplesamlphp/cert vendor/simplesamlphp/simplesamlphp/cert",
            "rm -rf vendor/simplesamlphp/simplesamlphp/metadata && ln -sf ../../../private/simplesamlphp/metadata vendor/simplesamlphp/simplesamlphp/metadata"
        ],
        "post-update-cmd": [
            "rm -rf vendor/simplesamlphp/simplesamlphp/config && ln -sf ../../../private/simplesamlphp/config vendor/simplesamlphp/simplesamlphp/config",
            "rm -rf vendor/simplesamlphp/simplesamlphp/cert && ln -sf ../../../private/simplesamlphp/cert vendor/simplesamlphp/simplesamlphp/cert",
            "rm -rf vendor/simplesamlphp/simplesamlphp/metadata && ln -sf ../../../private/simplesamlphp/metadata vendor/simplesamlphp/simplesamlphp/metadata"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/composer/{$name}": ["type:drupal-module"],
            "web/profiles/composer/{$name}": ["type:drupal-profile"],
            "web/themes/composer/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/private/scripts/quicksilver/{$name}/": ["type:quicksilver-script"]
        },
        "patches": {
            "drupal/inline_entity_form": {
                "Can't add more than two items with simple inline entity form widget under some circumstances": "https://www.drupal.org/files/issues/2019-11-04/3088901-3-cant-add-more-than-two-items.patch"
            },
            "drupal/protected_pages": {
                "Add template to protected pages": "https://www.drupal.org/files/issues/2020-10-09/3035962-6.patch"
            }
        },     
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false
            }
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/console-extend-plugin": true,
            "simplesamlphp/composer-module-installer": true,
            "oomphinc/composer-installers-extender": true,
            "rvtraveller/qs-composer-installer": true
        }
    },
    "version": "1.9.4.4"
}
