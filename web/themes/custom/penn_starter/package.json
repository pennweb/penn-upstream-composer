{
	"name": "penn-start-theme",
	"description": "Customizable upstream starter theme",
	"version": "0.1.0",
	"license": "MIT",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git://github.com/WDGDC/wordpress-theme.git"
	},
	"author": {
		"name": "University Communications",
		"url": "http://penntoday.upenn.edu/about-contact-us"
	},
	"engines": {
		"node": "^10.16.0"
	},
	"browserslist": [
		"last 2 versions",
		"ie 11"
	],
	"scripts": {
		"build": "gulp build",
		"build:css": "gulp build:css",
		"build:js": "gulp build:js",
		"build:vendor": "gulp build:vendor",
		"build:polyfill": "gulp build:polyfill",
		"build:prod": "gulp build:css && gulp build:js && gulp build:js && gulp build:vendor && gulp build:polyfill && grunt twigRender && gulp clean:twig && grunt move",
		"gulp": "gulp",
		"start": "npm run gulp",
		"templates": "concurrently \"npm run gulp\" \"npm run watch:twig\"",
		"twig": "grunt twigRender && grunt move",
		"watch": "gulp watch",
		"watch:sync": "gulp watch:sync",
		"watch:twig": "onchange \"components/**/*.twig\" \"components/**/*.json\" -- npm run twig"
	},
	"devDependencies": {
		"ansi-colors": "^4.1.1",
		"bows": "^1.7.0",
		"color-sass": "^3.0.0",
		"concurrently": "^4.1.2",
		"del": "5.0.0",
		"fancy-log": "^1.3.3",
		"grunt": "^1.0.4",
		"grunt-move": "^1.0.3",
		"grunt-twig-render": "^1.8.2",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "7.0.0",
		"gulp-cli": "2.2.0",
		"gulp-concat": "^2.6.1",
		"gulp-csso": "3.0.1",
		"gulp-filter": "6.0.0",
		"gulp-header": "2.0.9",
		"gulp-html-prettify": "0.0.1",
		"gulp-load-plugins": "2.0.1",
		"gulp-plumber": "1.2.1",
		"gulp-rename": "1.4.0",
		"gulp-sass": "^4.0.2",
		"gulp-sass-glob": "1.1.0",
		"gulp-size": "3.0.0",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-uglify": "3.0.2",
		"headroom.js": "^0.11.0",
		"hoverintent": "^2.2.1",
		"inflection": "^1.12.0",
		"lodash": "^4.17.15",
		"lodash.throttle": "^4.1.1",
		"object-fit-images": "^3.2.4",
		"onchange": "^6.1.0",
		"pretty-bytes": "5.3.0",
		"rollup": "1.20.0",
		"rollup-plugin-buble": "0.19.8",
		"rollup-plugin-commonjs": "^10.0.2",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-uglify": "^6.0.4",
		"rollup-stream": "^1.24.1",
		"uppercamelcase": "3.0.0",
		"vinyl-buffer": "^1.0.1",
		"vinyl-paths": "3.0.0",
		"vinyl-source-stream": "^2.0.0"
	},
	"dependencies": {
		"clipboard": "^2.0.4",
		"jquery": "^3.5.1",
		"picturefill": "^3.0.3",
		"prismjs": "^1.17.1"
	}
}
